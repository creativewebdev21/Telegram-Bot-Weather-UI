{
  "name": "nestnext8",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server/main.ts",
    "build:clean": "rimraf .next",
    "build:ui": "npx next build",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\"",
    "build": "yarn build:clean && yarn build:ui && yarn build:server",
    "start": "cross-env NODE_ENV=production node .next/production-server/server/main.js",
    "prettier:fix": "npx prettier --write components pages providers server",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "lint:fix": "next lint --fix",
    "lint": "next lint --max-warnings 0"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@react-oauth/google": "^0.11.1",
    "@tailwindcss/forms": "^0.5.6",
    "aos": "^2.3.4",
    "axios": "^1.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "mongoose": "^7.6.3",
    "nest-next": "^9.6.0",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sweetalert": "^2.1.2",
    "tailwind-scrollbar": "^3.0.5",
    "telegraf": "^4.14.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@types/aos": "^3.0.5",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.13.0",
    "eslint-config-next": "^13.5.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.31",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
